
  name: busybox-pod
spec:
  containers:
  - name: busybox
    image: busybox
    command:
      - "cat"
      - "/data/message"
    volumeMounts:
    - name: configmap-volume
      mountPath: /data
  volumes:
  - name: configmap-volume
    configMap:
      name: my-configmap
      items:
      - key: message
        path: message
